{

  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Warning"
    },
    "WriteTo": [
      {
        "Name": "Console"
      },

      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "https://seq.roboost.app",
          "eventBodyLimitBytes": 100000

        }
      }
    ],
    "Properties": {
      "ApplicationName": "Instashop"
    }
  }
}